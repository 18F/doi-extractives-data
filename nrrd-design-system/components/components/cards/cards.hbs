{{#each cards}}
<article class="card">
  <a href="#" class="card-image_link"><img class="card-image" src="{{ path card-image }}" alt="{{ card-alt }}"></a>
  <div class="card-content">
    <p class="card-content-subhead">{{ section-title }}</p>
    <a href="#"><h3 class="card-title">{{ card-title }}</h3></a>
    <p>{{ card-content }}</p>
  </div>
</article>
{{/each}}
